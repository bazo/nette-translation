{
	"name": "bazo/nette-translation",
	"type": "nette-addon",
	"description": "Translation extension for Nette Framework",
	"keywords": ["nette", "neon", "translation"],
	"homepage": "https://github.com/bazo/nette-translation",
	"authors": [
		{
			"name": "Martin Bažík",
			"email": "martin@bazo.sk",
			"homepage": "http://bazo.sk"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"nikic/php-parser": "*",
		"symfony/console": "*",
		"nette/neon": "*",
		"vipsoft/unzip": "@dev"
	},
	"suggest": {
		"bazo/nette-console-extension": "Running the CLI commands",
		"kdyby/console": "Running the CLI commands"
	},
	"autoload": {
		"psr-0": {"Bazo\\Translation": "src/"}
	}
}